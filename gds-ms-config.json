{
    "name": "gds-ms-project",
    "path": "/home/gds-server/output",
    "containers": {
        "templates": {
            "dockerTemplateFile": "./templates/docker-template.json",
            "scripts": {
                "windowScriptsTemplateFile": "./templates/scripts-windows-template.json",
                "unixScriptsTemplateFile": "./templates/scripts-unix-template.json"
            }
        },
        "serviceDocker": {
            "FROM": "rickzx98/gds-ms:#IMAGE_SERVICE_TAG",
            "MAINTAINER": "rickzx98@gmail.com",
            "RUN[0]": "apt-get update",
            "RUN[1]": "git clone #REPO /host && npm config set logfd /app/log/system.log && mkdir /app/scripts/",
            "WORKDIR": "/host",
            "RUN[2]": "npm install",
            "ENV[0]": "PORT 3000",
            "ENV[1]": "APP_LOG /app/log/system.log",
            "ENV[2]": "DB_HOST localhost",
            "ENV[3]": "DB service_db",
            "ENV[4]": "DB_TEST service_db_test",
            "ENV[5]": "DB_PORT 27017",
            "ENV[6]": "DB_PORT_TEST 27017",
            "ENV[7]": "DB_HOST_TEST localhost",
            "CMD": "mongod --fork --logpath=/app/db/log/mongod.log && npm start",
            "EXPOSE": "3000"
        },
        "services": [{
            "priority": "10",
            "name": "user-service",
            "parameters": {
                "#DOMAIN_SERVICE": "user_service",
                "#DBPATH": "/home/gds-server/output/user-db/",
                "#REPO": "https://github.com/great-design-and-systems/user-service.git",
                "#MAINTAINER": "rickzx98@gmail.com",
                "#IMAGE_SERVICE_TAG": "0.2"
            },
            "mounts": {
                "/app/log": "/home/gds-server/output/user-log/",
                "/data/db": "/home/gds-server/output/user-db/",
                "/app/scripts/": "/home/gds-server/output/scripts/"
            }
        }, {
            "priority": "10",
            "name": "security-service",
            "parameters": {
                "#DOMAIN_SERVICE": "security_service",
                "#DBPATH": "/home/gds-server/output/security-db/",
                "#REPO": "https://github.com/great-design-and-systems/security-service.git",
                "#MAINTAINER": "rickzx98@gmail.com",
                "#IMAGE_SERVICE_TAG": "0.2",
                "#SCHOOL_CONFIG_SERVICE_PORT": "NONE"
            },
            "mounts": {
                "/app/log": "/home/gds-server/output/security-log/",
                "/data/db": "/home/gds-server/output/security-db/",
                "/app/scripts/": "/home/gds-server/output/scripts/"
            }
        }, {
            "priority": "10",
            "name": "faculty-service",
            "parameters": {
                "#DOMAIN_SERVICE": "faculty_service",
                "#DBPATH": "/home/gds-server/output/faculty-db/",
                "#REPO": "https://github.com/great-design-and-systems/faculty-service.git",
                "#MAINTAINER": "rickzx98@gmail.com",
                "#IMAGE_SERVICE_TAG": "0.2"
            },
            "mounts": {
                "/app/log": "/home/gds-server/output/faculty-log/",
                "/data/db": "/home/gds-server/output/faculty-db/",
                "/app/scripts/": "/home/gds-server/output/scripts/"
            }
        }, {
            "priority": "10",
            "name": "student-service",
            "parameters": {
                "#DOMAIN_SERVICE": "student_service",
                "#DBPATH": "/home/gds-server/output/student-db/",
                "#REPO": "https://github.com/great-design-and-systems/student-service.git",
                "#MAINTAINER": "rickzx98@gmail.com",
