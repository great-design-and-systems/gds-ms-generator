                "#REPO": "https://github.com/great-design-and-systems/gds-ms-api.git",
                "#MAINTAINER": "rickzx98@gmail.com",
                "#IMAGE_SERVICE_TAG": "0.2"
            },
            "environments": {
                "SCHOOL_CONFIG_SERVICE_PORT": "https://school-config-service.herokuapp.com"
            },
            "links": [
                "user_service",
                "security_service",
                "faculty_service",
                "time_service",
